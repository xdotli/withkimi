{
  "name": "withkimi",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "supabase"
  ],
  "scripts": {
    "_with-env": "dotenv -e .env -c -- ",
    "ios": "cd apps/expo && yarn ios",
    "android": "cd apps/expo && yarn android",
    "native": "cd apps/expo && yarn start",
    "watch": "yarn check:type:watch & yarn workspaces foreach -pi --all run watch",
    "web": "yarn build && cd apps/next && yarn dev --hostname 0.0.0.0",
    "web:extract": "DISABLE_EXTRACTION=false yarn workspace next-app dev",
    "web:prod": "yarn workspace next-app build",
    "web:prod:analyze": "ANALYZE=true yarn web:prod",
    "web:prod:serve": "yarn workspace next-app serve",
    "fix": "manypkg fix",
    "postinstall": "yarn check-deps && yarn build",
    "build": "yarn workspaces foreach --all --exclude next-app run build",
    "upgrade:tamagui": "yarn up '*tamagui*' '@tamagui/*'",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary",
    "check-deps": "check-dependency-version-consistency . --ignore-dep react-native-reanimated",
    "supa": "yarn workspace @my/supabase",
    "typecheck": "turbo check:type",
    "check:type": "tsc -b --preserveWatchOutput tsconfig.json",
    "check:type:watch": "yarn check:type -w",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "setup": "yarn create tamagui --template takeout-starter --skip-cloning",
    "generate": "turbo gen",
    "gen": "yarn generate",
    "release:version": "changeset status & changeset version",
    "release:publish": "changeset publish"
  },
  "resolutions": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "react-native-svg": "13.9.0",
    "react-native-web": "~0.19.6",
    "@babel/core": "^7.20.2",
    "babel-loader": "^8.3.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@changesets/cli": "^2.27.1",
    "@manypkg/cli": "^0.19.1",
    "@nderscore/tamagui-typescript-plugin": "^0.6.0",
    "@tamagui/cli": "^1.85.12",
    "@turbo/gen": "^1.10.12",
    "@types/eventsource": "^1.1.15",
    "change-case": "^4.1.2",
    "check-dependency-version-consistency": "^3.0.3",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "elevenlabs": "^0.1.1",
    "eslint": "^8.46.0",
    "eventsource": "^2.0.2",
    "lottie-react-native": "^6.5.1",
    "node-fetch": "^3.3.2",
    "node-gyp": "^9.3.1",
    "openai": "^4.25.0",
    "prettier": "^2.7.1",
    "react-native-date-picker": "^4.3.5",
    "turbo": "^1.10.14",
    "typescript": "^5.3.3"
  },
  "packageManager": "yarn@4.0.2",
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "patch-package": "^8.0.0"
  }
}
