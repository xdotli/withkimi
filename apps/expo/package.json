{
  "name": "expo-app",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "clean": "react-native-clean-project",
    "start": "EXPO_USE_METRO_WORKSPACE_ROOT=1 npx expo start -c",
    "android": "yarn expo run:android",
    "eas:build:dev:ios": "cross-env APP_ENV=development eas build --profile development --platform ios",
    "eas:submit:dev:ios": "cross-env APP_ENV=development eas submit --profile staging",
    "eas:update:dev:ios": "cross-env APP_ENV=development eas update --channel staging",
    "eas:build:staging:ios": "cross-env APP_ENV=staging eas build --profile staging --platform ios",
    "eas:submit:staging:ios": "cross-env APP_ENV=staging eas submit --profile staging",
    "eas:update:staging:ios": "cross-env APP_ENV=staging eas update --channel staging",
    "ios": "yarn expo run:ios",
    "lint": "eslint ./app",
    "lint:fix": "eslint ./app --fix",
    "eject": "expo eject"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@my/ui": "*",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/netinfo": "9.3.10",
    "app": "*",
    "babel-plugin-module-resolver": "^4.1.0",
    "burnt": "^0.12.1",
    "eslint-plugin-import": "^2.28.0",
    "expo": "^49.0.0",
    "expo-apple-authentication": "~6.1.0",
    "expo-build-properties": "~0.8.3",
    "expo-constants": "~14.4.2",
    "expo-crypto": "^12.6.0",
    "expo-dev-client": "~2.4.6",
    "expo-device": "~5.4.0",
    "expo-font": "~11.4.0",
    "expo-image": "~1.3.3",
    "expo-image-picker": "~14.3.1",
    "expo-linear-gradient": "~12.3.0",
    "expo-linking": "~5.0.2",
    "expo-notifications": "~0.20.1",
    "expo-router": "2.0.0",
    "expo-secure-store": "~12.3.1",
    "expo-splash-screen": "~0.20.4",
    "expo-status-bar": "~1.6.0",
    "expo-updates": "~0.18.13",
    "expo-web-browser": "^12.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.72.5",
    "react-native-dotenv": "^3.4.8",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@expo/metro-config": "~0.10.0",
    "@tamagui/babel-plugin": "^1.84.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "cross-env": "^7.0.3",
    "metro-minify-terser": "^0.74.1",
    "react-native-clean-project": "^4.0.1",
    "typescript": "^5.2.2"
  }
}
