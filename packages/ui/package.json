{
  "name": "@my/ui",
  "version": "0.0.1",
  "sideEffects": [
    "*.css"
  ],
  "private": true,
  "types": "./src",
  "main": "src/index.tsx",
  "module:jsx": "src",
  "files": [
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build --skip-types",
    "watch": "tamagui-build --skip-types --watch",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "generate-theme": "tamagui generate-themes ./src/themes/theme.ts ./src/themes/theme-generated.ts",
    "check:type": "tsc --outDir /tmp",
    "check:type:watch": "yarn check:type -w"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.0",
    "@tamagui/animations-moti": "^1.85.12",
    "@tamagui/animations-react-native": "^1.85.12",
    "@tamagui/cli": "^1.85.12",
    "@tamagui/create-theme": "^1.85.12",
    "@tamagui/font-inter": "^1.85.12",
    "@tamagui/helpers-icon": "^1.85.12",
    "@tamagui/react-native-media-driver": "^1.85.12",
    "@tamagui/shorthands": "^1.85.12",
    "@tamagui/theme-builder": "^1.85.12",
    "@tamagui/themes": "^1.85.12",
    "@tamagui/toast": "^1.85.12",
    "@ts-react/form": "1.6.4",
    "moti": "^0.25.3",
    "react-hook-form": "^7.46.1",
    "tamagui": "^1.85.12",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@tamagui/build": "^1.85.12",
    "eslint": "^8.46.0",
    "eslint-config-custom": "*",
    "typescript": "^5.3.3"
  }
}
